record (ao, "PYEPICS_DEMO:SETPOINT") {
  field (VAL, "7")
  field (PINI, "YES")
  field (DRVL, 0)
  field (DRVH, 10)
  field (LOPR, 0)
  field (HOPR, 10)
}


record (sub, "PYEPICS_DEMO:READBACK") {
  field (SCAN, ".1 second")
  field (SNAM, "processReadback")
  field (INPA, "PYEPICS_DEMO:SETPOINT")
  field (PREC, 3)
}


record (calc, "PYEPICS_DEMO:CALC") {
  field (DESC, "Example calc")
  field (SCAN, "Passive")
  field (PINI, "YES")
  field (INPA, "2")
  field (INPB, "3")
  field (CALC, "A * B")
  field (EGU, "mA")
}


record (sub, "PYEPICS_DEMO:TEMPERATURE") {
  field (SCAN, "1 second")
  field (INAM, "initSubroutine")
  field (SNAM, "processSubroutine")
  field (PREC, 3)
}


record (aSub, "PYEPICS_DEMO:WAVE_GENERATOR") {
  field (SCAN, "1 second")
  field (SNAM, "processWave")
  field (FTVA, "FLOAT")
  field (NOVA, "1000")
  field (OUTA, "PYEPICS_DEMO:WAVE PP")
}


record (waveform, "PYEPICS_DEMO:WAVE") {
  field (FTVL, "FLOAT")
  field (NELM, "1000")
}


record (mbbo, "PYEPICS_DEMO:COLOUR") {
  field (VAL, "0")
  field (PINI, "YES")
  field (ZRST, "red")
  field (ONST, "green")
  field (TWST, "blue")
  field (ZRSV, "MAJOR")
  field (ONSV, "NO_ALARM")
  field (TWSV, "NO_ALARM")
}
